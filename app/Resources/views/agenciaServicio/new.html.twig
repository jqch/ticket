{% extends 'layout.html.twig' %}
{% block header %}
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="{{path('dashboard')}}">Dashboard</a>
        </li>
        <li class="">Servicios Agencia</li>
        <li class="active">Crear</li>
    </ul><!-- /.breadcrumb -->
{% endblock %}
{% block body %}
    <div class="page-header">
        <h1>
            Servicios Agencia
            <small>
                <i class="ace-icon fa fa-angle-double-right"></i>
                Asignar nuevo servicio
            </small>
        </h1>
    </div>
    <div class="clearfix">
            <div class="pull-right tableTools-container">
                <div class="dt-buttons btn-overlap btn-group">
                    <a href="{{ path('agenciaservicio_index') }}" class="dt-button buttons-collection buttons-colvis btn btn-white btn-primary btn-bold"><i class="fa fa-hand-o-left bigger-110 blue"></i></a>
                </div>
            </div>
    </div>
    <div class="table-header">
        Nueva asignacion de servicios
    </div>
        <div class="col-lg-12 col-md-12">
                <form action="{{path('agenciaservicio_new')}}" method="post" class="form-horizontal">
                    <div class="form-group">
                        <label for="">{{form_label(form.agencia)}}</label>
                        {{form_widget(form.agencia)}}
                    </div>
                    <div class="form-group">
                        <label for="">{{form_label(form.servicio)}}</label>
                        {{form_widget(form.servicio)}}
                    </div>
                    <hr>
                    <button type="submit" class="btn btn-white btn-info btn-bold"><i class="fa fa-save green"></i> Registrar</button>
                </form>
        </div>
{% endblock %}
